{
  "name": "im-electron-demo",
  "productName": "IM Electron Demo",
  "version": "1.0.0",
  "description": "IM Electron Demo",
  "main": "src/app/main.js",
  "scripts": {
    "start": "concurrently \"npm run dev:electron\"",
    "build": "npm run build:react && electron-builder -w",
    "build:react": "cd ./src/client && npm run build:react",
    "dev:electron": "electron .",
    "dev:react": "cd ./src/client && npm run dev:react",
    "dev": "npm run dev:react && npm run start"
  },
  "keywords": [],
  "author": {
    "name": "Jingfeng",
    "email": "joyxian@tencent.com"
  },
  "build": {
    "files": [
      "src/app",
      "bundle"
    ],
    "appId": "electron-sdk",
    "mac": {},
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "extraFiles": [
        {
          "from": "./node_modules/im_electron_sdk/lib/",
          "to": "./node_modules/im_electron_sdk/lib/",
          "filter": [
            "**/*"
          ]
        }
      ],
      "extraResources": [
        "./extraResources/**"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "electron": "^13.1.5",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "braft-editor": "^2.3.9",
    "concurrently": "^6.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "element-react": "^1.4.34",
    "element-theme-default": "^1.4.13",
    "file-loader": "^6.2.0",
    "final-form": "^4.20.2",
    "im_electron_sdk": "^1.0.33",
    "js-audio-recorder": "^1.0.7",
    "lodash": "^4.17.21",
    "qs": "^6.5.2",
    "react": "^17.0.2",
    "react-contexify": "^5.0.0",
    "react-contextmenu": "^2.14.0",
    "react-cropper": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.2",
    "react-loading-skeleton": "^2.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "sass": "^1.35.1",
    "tea-component": "^2.6.22",
    "trtc-electron-sdk": "^8.6.102-beta.0"
  }
}
