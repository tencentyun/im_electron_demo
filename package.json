{
  "name": "oaim",
  "productName": "oaim",
  "version": "1.0.40",
  "description": "员工工作平台",
  "main": "src/app/main.js",
  "scripts": {
    "start:dev": " set NODE_ENV=development && set HUARUN_ENV=dev && concurrently \"npm run dev:react\" \"npm run start-electron\"",
    "start:prod": "set NODE_ENV=production && set HUARUN_ENV=prod && concurrently \"npm run dev:react\" \"npm run start-electron\"",
    "start:test": "set NODE_ENV=production && set HUARUN_ENV=test && concurrently \"npm run dev:react\" \"npm run start-electron\"",
    "build:prod": "set NODE_ENV=production && set HUARUN_ENV=prod && npm run build:react && npm run build-electron",
    "build:test": "set NODE_ENV=production && set HUARUN_ENV=test && npm run build:react && npm run build-electron",
    "build:dev": "set NODE_ENV=production && set HUARUN_ENV=dev && npm run build:react && npm run build-electron",
    "build:react": "cd ./src/client && npm run build",
    "dev:react": "cd ./src/client && npm run dev",
    "start-electron": "electron .",
    "build-electron": "electron-builder -w"
  },
  "keywords": [],
  "author": {
    "name": "珠海华润银行",
    "email": "yangguang@crbank.com.cn"
  },
  "build": {
    "files": [
      "src/app",
      "bundle"
    ],
    "productName": "员工工作平台",
    "appId": "huarunim",
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "runAfterFinish": true,
      "shortcutName": "员工工作平台"
    },
    "extraFiles": [
      "node_modules/trtc-electron-sdk/build/Release/trtc_electron_sdk.node"
    ],
    "mac": {
      "icon": "./icon/favicon.icns"
    },
    "win": {
      "icon": "./icon/windows.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ],
      "extraFiles": [
        {
          "from": "./node_modules/im_electron_sdk/lib/",
          "to": "./resources",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "node_modules/trtc-electron-sdk/build/Release/",
          "to": "./resources",
          "filter": [
            "**/*"
          ]
        }
      ],
      "extraResources": [
        "./extraResources/**"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron": "^13.2.1",
    "electron-builder": "^22.11.7",
    "install": "^0.13.0",
    "native-ext-loader": "^2.3.0",
    "npm": "^7.20.6"
  },
  "dependencies": {
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "ffi-napi": "^4.0.3",
    "ffmpeg-extract-frame": "^1.0.2",
    "ffprobe": "^1.1.2",
    "file-type": "^16.5.2",
    "fluent-ffmpeg": "^2.1.2",
    "im_electron_sdk": "^1.2.32",
    "image-size": "^1.0.0",
    "node-fetch": "^2.6.1",
    "node-gyp": "^7.1.2",
    "node-gyp-build": "^4.2.3",
    "progress-stream": "^2.0.0",
    "qs": "^6.5.2",
    "sass": "^1.35.1",
    "save": "^2.4.0",
    "trtc-electron-sdk": "^8.9.107-beta.0"
  }
}
