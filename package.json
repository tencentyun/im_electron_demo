{
  "name": "oaim",
  "productName": "oaim",
  "version": "1.0.35",
  "description": "华润银行及时通讯（准生产）",
  "main": "src/app/main.js",
  "scripts": {
    "start:dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "start:prod": "concurrently \"npm run prod:react\" \"npm run prod:electron\"",
    "start:test": "concurrently \"npm run test:react\" \"npm run test:electron\"",
    "build:prod": "concurrently \"npm run build-client-prod\" \"npm run build-electron-prod\"",
    "build:test": "npm run build:react:test && cross-env NODE_ENV=production huarun_env=test npm run build-electron",
    "build:dev": "npm run build:react:dev && cross-env NODE_ENV=production huarun_env=dev npm run build-electron",
    "build:react:prod": "cross-env NODE_ENV=production huarun_env=prod npm run open-build",
    "build:react:test": "cd ./src/client && npm run build:react:test",
    "build:react:dev": "cd ./src/client && npm run build:react:dev",
    "dev:electron": "cross-env NODE_ENV=development huarun_env=dev npm run start-electron",
    "dev:react": "cd ./src/client && npm run dev:react",
    "prod:electron": "cross-env NODE_ENV=development huarun_env=prod npm run start-electron",
    "prod:react": "cd ./src/client && npm run prod:react",
    "test:electron": "cross-env NODE_ENV=development huarun_env=test npm run start-electron",
    "test:react": "cd ./src/client && npm run test:react",
    "start-electron": "electron .",
    "build-electron": "electron-builder -w",
    "build-prod":"npm run build:react:prod &&  npm run build-electron",
    "open-build": "cd ./src/client && npm run build:react:prod",
    "build-client-prod":"cross-env huarun_env=prod npm run build:react:prod",
    "build-electron-prod":"set NODE_ENV=production && electron-builder -w"

  },
  "keywords": [],
  "author": {
    "name": "珠海华润银行",
    "email": "yangguang@crbank.com.cn"
  },
  "build": {
    "files": [
      "src/app",
      "bundle"
    ],
    "productName": "员工工作平台",
    "appId": "electron-sdk",
    "mac": {
      "icon": "./icon/favicon.icns"
    },
    "win": {
      "icon": "./src/client/assets/icon/windows.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "extraFiles": [
        {
          "from": "./node_modules/im_electron_sdk/lib/",
          "to": "./resources",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "node_modules/trtc-electron-sdk/build/Release/",
          "to": "./resources",
          "filter": [
            "**/*"
          ]
        }
      ],
      "extraResources": [
        "./extraResources/**"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron": "^13.1.5",
    "electron-builder": "^22.11.7",
    "native-ext-loader": "^2.3.0"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "ffmpeg-extract-frame": "^1.0.2",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.0.0",
    "file-type": "^16.5.2",
    "fluent-ffmpeg": "^2.1.2",
    "get-video-duration": "^3.1.0",
    "im_electron_sdk": "^1.2.7",
    "image-size": "^1.0.0",
    "node-fetch": "^2.6.1",
    "node-gyp": "^7.1.2",
    "node-gyp-build": "^4.2.3",
    "npm": "^7.20.2",
    "progress-stream": "^2.0.0",
    "qs": "^6.5.2",
    "sass": "^1.35.1",
    "save": "^2.4.0",
    "trtc-electron-sdk": "^8.9.107-beta.0"
  }
}
