{
  "name": "im-electron-demo",
  "productName": "IM Electron Demo",
  "version": "1.0.0",
  "description": "IM Electron Demo",
  "main": "src/app/main.js",
  "scripts": {
    "start": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "build": "npm run build:react && set NODE_ENV=production && electron-builder -w",
    "build:react": "cd ./src/client && npm run build:react --target_platform=win32",
    "dev:electron": "set NODE_ENV=development && electron .",
    "dev:react": "cd ./src/client && npm run dev:react"
  },
  "keywords": [],
  "author": {
    "name": "Jingfeng",
    "email": "joyxian@tencent.com"
  },
  "build": {
    "files": [
      "src/app",
      "bundle"
    ],
    "appId": "electron-sdk",
    "mac": {},
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "extraFiles": [
        {
          "from": "./node_modules/im_electron_sdk/lib/",
          "to": "./node_modules/im_electron_sdk/lib/",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "node_modules/trtc-electron-sdk/build/Release/",
          "to": "./resources",
          "filter": ["**/*"]
        }
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "electron": "^13.1.5",
    "electron-builder": "^22.11.7",
    "native-ext-loader": "^2.3.0",
    "node-gyp": "^7.1.2"
  },
  "dependencies": {
    "im_electron_sdk": "^1.0.36",
    "trtc-electron-sdk": "^8.6.102-beta.0"
  }
}
